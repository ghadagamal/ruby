

<!DOCTYPE html>
<html>
<head>
  <title>G</title>
  <%= stylesheet_link_tag    'application', media: 'all', 'data-turbolinks-track' => true %>
  <%= javascript_include_tag 'application', 'data-turbolinks-track' => true %>
  <%= csrf_meta_tags %>

</head>
<body>


   <nav class="navbar navbar-default">
  <div class="container-fluid">
    <!-- Brand and toggle get grouped for better mobile display -->
    <div class="navbar-header">
      <button type="button" class="navbar-toggle collapsed" data-toggle="collapse" data-target="#bs-example-navbar-collapse-1" aria-expanded="false">
        <span class="sr-only">Toggle navigation</span>
        <span class="icon-bar"></span>
        <span class="icon-bar"></span>
        <span class="icon-bar"></span>
      </button>

      
     
    
   <%= link_to "course",courses_path , class: "btn btn-default navbar-btn" ,:style=>'color:#000000;  text-decoration:none';%> 

<% if user_signed_in? %>
    
     <%= link_to "Sign out", destroy_user_session_path, :method => :delete, class: "btn btn-default navbar-btn" ,:style=>'color:#000000;  text-decoration:none';%> 
     
     <%= link_to "createcourse",new_course_path , class: "btn btn-default navbar-btn" ,:style=>'color:#000000;  text-decoration:none';%> 
    <%= link_to "createlecture",new_lecture_path , class: "btn btn-default navbar-btn" ,:style=>'color:#000000;  text-decoration:none';%> 
    <%= link_to "homepage",root_path , class: "btn btn-default navbar-btn" ,:style=>'color:#000000;  text-decoration:none';%> 
 <%= link_to "profile",pages_path , class: "btn btn-default navbar-btn" ,:style=>'color:#000000;  text-decoration:none';%> 
 <%else%>
   
     <%= link_to "login",  new_user_session_path, class: "btn btn-default navbar-btn"%> 
<%end%>


</div>
</div>
</nav>




<%= yield %>

</body>
</html>


